<script language="JavaScript">
	function trim(s)	{
		return s.replace(/^\s+|\s+$/, '');
	}

	function input_required(fval)	{
		fval = trim(fval);
		if (fval==null||fval=="")
		{
			return false;
		}
		else 
			return true;
	}

	function checkInput() 	{
		if (input_required(document.getElementById('keyWord').value)==false)
		{
			alert('Keyword cannot be blank');
			document.getElementById('keyWord').focus();
			return false;
		}
		return true;
	}

	function DropDownSelect(obj, val)
	{
		var i;
		var len = obj.options.length;	
		for (i=0; i<len; i++)
		{
			if (obj.options[i].value == val)
			{
				obj.selectedIndex = i;
				break;
			}
		}
	}
</script>


