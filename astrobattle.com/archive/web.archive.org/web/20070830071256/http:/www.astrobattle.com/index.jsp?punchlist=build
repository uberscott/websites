		zoomWindow.close();
		zoomWindow = window.open('', 'zoomWin', 'width='+imgWidth+',height='+imgHeight+'top=10,left=5,toolbar=0,menubar=0,location=0,scrollbars=0')
		zoomWindow.document.write("<html><head><title>Loading Image, Please Wait...<\/title><\/head><body background='/img/popupbacking.gif' leftmargin=0 topmargin=0 marginwidth=0 marginheight=0 text='",imgWidth,imgHeight,"' onBlur='close();'><center><a href='#' onClick =javascript:window.close()><img src='",imgName,"' alt='Click to Close' border='0'><\/a><\/center><\/body><\/html>")
		zoomWindow.focus()
		zoomWindow.window.resizeTo(imgWidth+8, imgHeight+29);
		zoomWindow.document.title = "Astro Battle"
	}


