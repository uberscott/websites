var g_g= new Array(0);
var g_i= new Array(0);
var g_n= new Array(0);
var g_ri= new Array(0);
function frmsub(){
	var f_n = document.SearchFormTop.searchrequest;
	if (f_n.value.length < 1){
		alert("Please enter some keywords for your search.");
		f_n.focus();
		return false;
	}
	if ((f_n.value.search(/[^ \t\r\n\f]/gi)<0)){
		alert("Your search phrase is blank or only contains spaces.");
		f_n.focus();
		return false;
	}
	document.SearchFormTop.action=document.SearchFormTop.action+"&f_search_criteria="+ f_n.value;
	return true;
	}
function bkgadd(g,i,n){
	g_g[g_g.length]=g;
	g_i[g_i.length]=i;
	g_n[g_n.length]=n;
}
function bkrndi(c,n){
	var f_n=0;
	var f_f=false;
	g_ri= new Array(0);
	if (n>c) n=c;
	while (g_ri.length<n){
		f_n=Math.floor(c*Math.random());
		f_f=false;
		for(i=0;i<g_ri.length;i++){
			if (g_ri[i]==f_n)
				f_f=true;
			}
		if (!f_f)
			g_ri[g_ri.length]=f_n;
		}
}
function bkrnd(){
	var f_n = document.getElementById('featured_book_groups');
	var f_s = '';
	bkrndi(g_g.length,3);
	for(i=0;i<g_ri.length;i++){
		f_s=f_s+ '<li><a href="https://web.archive.org/web/20071024175056/http://groups.myspace.com/index.cfm?fuseaction=groups.groupProfile&groupID='+g_g[g_ri[i]]+
			'">'+g_n[g_ri[i]]+
			'</a><br /><a href="https://web.archive.org/web/20071024175056/http://groups.myspace.com/index.cfm?fuseaction=groups.groupProfile&groupID='+g_g[g_ri[i]]+
			'"><img src="'+g_i[g_ri[i]]+'" /></a></li>';
	}
	f_n.innerHTML='<ul>'+f_s+'</ul>';
}

	
		
