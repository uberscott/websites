<script type="text/javascript">
function changeCulture(popuptxt,type,targetURL)
{ 
    var result =false;
	if(type == 'alert')
	    alert(popuptxt);
	else
	    result = confirm(popuptxt);
	    
    var currenturl = window.location.href;
   	var persist = document.getElementById("hdnPersist");
    var path = window.location.pathname;
    
    //only one default.aspx needed.
    if(path.indexOf("default.aspx/")>0)
        targetURL += path.substring(path.indexOf("default.aspx/")+13,path.length);
    else if(path.indexOf("default.aspx")>0)
        targetURL += path.substring(path.indexOf("default.aspx")+12,path.length);
    else   
        targetURL += path;
    //check for querystring
    if(currenturl.indexOf('?') > -1)
        targetURL += currenturl.substring(currenturl.indexOf('?'),currenturl.length);

    if (result)  
		persist.value = "true";
        
    document.langLink.action = targetURL;
    document.langLink.submit();
 }

</script>

