            <script type="text/javascript" language="JavaScript">
  
              function google_ad_request_done(google_ads)
              {
                if (google_ads == null || google_ads.length == 0) return;
              
                var i = 0;
                var ctl = null;
                var str = '';
              
                document.write('<style type="text/css"> #BottomSection { _float: none; _margin: 35px 0 0 0; _width:770px;} <\/style>');
              
                str += '<div class=googleafc style="margin: 0 15px 0 0; _margin: 0;"> <div class=horz> <div class=googlehdr>Sponsored Links<\/div> <div class=adcount4 id=gafc>';
                for (l = 0; l < 4; l++) {
                  if (google_ads[i] == null) break;
                  str += '<a onmouseover="window.status=\'go to ' + google_ads[i].visible_url + '\'; return true;" onmouseout="window.status=\'\';" href="' + google_ads[i].url + '" target=_blank>'
                      + '<strong>' + google_ads[i].line1 + '<\/strong>  ' 
                      + '<p>' + google_ads[i].line2 + ' ' + google_ads[i].line3 + '<\/p>'
                      + '<span>' + google_ads[i].visible_url + '<\/span>'
                      + '<\/a>';
                  i++;
                }
                str += ' <\/div><\/div><\/div>';
                document.write(str);
              
                var gafc = document.getElementById("gafc");
                if (gafc == null) return;
                gafc.className = gafc.className.replace("adcount4", "adcount" + google_ads.length);
              }
  
              var google_ad_client = "fim_myspace_careers_js";
              var google_encoding = "utf8";
              var google_ad_channel = "fim_myspace_careers,fim_myspace_united-states";
              var google_cust_ch = "";
              var google_max_num_ads = 4;
              var google_ad_type = "text";
              var google_language = "en";
              var google_adtest = "off";
              var google_ed = "";
              var google_ad_output = "js";
              var google_safe = "high";
            </script>
            <script type="text/javascript" language="JavaScript" src="https://web.archive.org/web/20071009012807js_/http://pagead2.googlesyndication.com/pagead/show_ads.js"></script>
            </div>

	<!-- end google ads -->
            
</div>

