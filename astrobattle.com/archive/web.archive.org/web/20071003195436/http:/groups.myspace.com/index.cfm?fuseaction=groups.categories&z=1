								<script language="JavaScript">
									function trim(s)	{
										return s.replace(/^\s+|\s+$/, '');
									}

									function input_required(fval)	{
										fval = trim(fval);
										if (fval==null||fval=="")
										{
											return false;
										}
										else
											return true;
									}

									function checkInput() 	{
										if (input_required(document.getElementById('Keyword').value)==false)
										{
											alert('Keyword cannot be blank');
											document.getElementById('Keyword').focus();
											return false;
										}
										return true;
									}

								</script>
							
