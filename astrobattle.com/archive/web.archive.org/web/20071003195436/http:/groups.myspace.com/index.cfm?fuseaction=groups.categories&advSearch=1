<script type="text/javascript">
function InsertWatermark(searchtype){
	if ("it-IT" != "ja-JP")
		return;
	var strWarning = "CurrOnlySearchEnglish";
	if (document.srch.q.value == strWarning)
		document.srch.q.value = '';
	switch (searchtype){
		case "tms": document.srch.q.value = strWarning; break;
	}
}
function ChangeAction(){
	var searchvalue = document.SearchFormHeader.searchrequest.value;
	if(document.SearchFormHeader.searchrequest.value !=="") {
		if (document.SearchFormHeader.searchtype[1].checked) {
			
				document.SearchFormHeader.action = "https://web.archive.org/web/20070805122340/http://searchresults.myspace.com/index.cfm?fuseaction=advancedFind.results&websearch=1&spotId=3&Mytoken=6695878B-2C86-4606-858DEE52667755E629606073";
			
		}
		else if (document.SearchFormHeader.searchtype[0].checked) {
			
				document.SearchFormHeader.action = "https://web.archive.org/web/20070805122340/http://searchresults.myspace.com/index.cfm?fuseaction=advancedFind.web&websearch=1&spotId=3&Mytoken=6695878B-2C86-4606-858DEE52667755E629606073";
			
		}
		return true;
	}
}

function full(vid){
	var fs = window.open( "https://web.archive.org/web/20070805122340/http://vids.myspace.com/index.cfm?fuseaction=vids.fullscreen&videoid=" +vid, "fsv",
		"toolbar=no,width=" +screen.availWidth  +",height=" +screen.availHeight +",status=no,resizable=yes,fullscreen=yes,scrollbars=no");
	fs.focus();
}
