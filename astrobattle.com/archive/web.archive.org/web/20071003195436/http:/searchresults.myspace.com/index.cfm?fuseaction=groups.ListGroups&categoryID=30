 function __addElection() {
                if (document.images) {
                    var _i = new Image();
                    _i.src = 'http://creative.myspacecdn.com/design/myspace/images/election_hover.jpg';
                }
                var star = 'http://creative.myspacecdn.com/impact/electionicon.gif';
                var _election = "";
                
                var _pItem = document.createElement("li");
                var leftNav = document.getElementById("leftNav");
                if (leftNav) {
                    _pItem.setAttribute('id','election');
                    _pItem.innerHTML = "<a href='http://www.myspace.com/impact'><div class='electionCheck'><img src='" + star + "' class='starIMG' margin='0' border='0' hspace='0' vspace='0' /></div>&nbsp;Election '08&nbsp;</a>";
                    leftNav.appendChild(_pItem);
                    }
            }

function __FixNavBar(){
 __addElection();

    if (!MySpace.ClientContext.IsLoggedIn && MySpace.ClientContext.FunctionalContext == "Splash") {
        var rightNav = document.getElementById("rightNav");
        if (rightNav) {
          if (rightNav.childNodes[0]) 
           rightNav.removeChild(rightNav.childNodes[0]);
        }
    }    
}

if (typeof(MySpace) != 'undefined') {
    if (typeof(MySpace.ClientContext) != 'undefined') {
        if (typeof(MySpace.ClientContext.FunctionalContext) != 'undefined') {
        __FixNavBar();        
        }
    }
}


