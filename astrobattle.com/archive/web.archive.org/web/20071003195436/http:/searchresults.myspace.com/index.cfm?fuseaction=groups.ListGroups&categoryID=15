<script type="text/javascript">
function setSelect(pID,pSelectedValue){
	var h=document.getElementById(pID);
	if (h.options==undefined) return false;
	for (var i=0; i<h.options.length; i++){
		if (h.options[i].value == pSelectedValue){
			h.selectedIndex = i;
			break;
		}//if
	}//for
}// fn: setSelect

function setRadio(pIdList,pSelectedValue){
	var aryIDs=pIdList.split(",");
	for (var i=0; i<aryIDs.length; i++){
		var h=document.getElementById(aryIDs[i]);
		if (h != null) h.checked=(h.value == pSelectedValue);
	}//for
}// fn: setRadio

function trim(i){i=i.replace(/([ \t\r\n\f])*$/gi,"");	return i.replace(/^([ \t\r\n\f])*/gi,"");}
</script>

<script type="text/javascript">
function handleCountryChange(pCount){
	var s=document.getElementById('advSrchStates');
	if (pCount < 1){
		s.options[0].text='N.A.';//';
		s.options[0].value=-1;
		s.disabled=true;
	}else{
		s.options[0].text='-- Any --';//';
		s.options[0].value=0;
		s.disabled=false;
	}

  var advSrchCountry=  document.getElementById('advSrchCountry');
  var advSrchDistance = document.getElementById('advSrchDistance');
  var advSrchZip = document.getElementById('advSrchZip');
  
  if(trim(advSrchCountry.value) != 'US'){	
		advSrchDistance.disabled = true;
		advSrchDistance.selectedIndex = 0;
		advSrchZip.disabled = true;
		advSrchZip.value = '';
	}
	else{
		advSrchDistance.disabled = false;
		advSrchZip.disabled = false;			
	}	
}
	
function chkSrch(){
	var h=document.getElementById("advSrchString");
	h.value= trim(h.value);
	if(h.value == ""){
		alert ("Enter either a group name or keyword in perform a search.");//");
		h.focus();
		return false;
	}
	return true;
}
</script>
