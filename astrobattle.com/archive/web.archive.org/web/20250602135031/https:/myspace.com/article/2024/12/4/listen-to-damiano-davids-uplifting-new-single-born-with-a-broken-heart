	<script src="//web.archive.org/web/20250530115619js_/https://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>

	<script>
	(function () {
		var context;

		Object.defineProperty(window, 'context', {
			set: function (value) {
				context = value;
				decode(context.beaconData);
				decode(context.beaconDataV3);
			},
			get: function () {
				return context;
			}
		});

		function decode(data) {
			data && Object.keys(data).forEach(function (key) {
				if (typeof data[key] === 'string') {
					try {
						data[key] = atob(data[key]);
					}
					catch (ex) {}
				}
				return data;
			});
		}
	})();
</script>

