<!-- Start of storage quota counter code -->
<script language="javascript">

var startQuota = 126698179.951;
var startTime = (new Date()).getTime();
var extraStoragePerDay = 1747626.66667;

function formatSpace(amount, pad) {
	// Show amount in megabytes
	amount /= 1024 * 1024;

  var str = String(amount);
  var dot = str.indexOf('.');

  if (dot < 0) {
    str = str + pad;
  } else if (pad.length > (str.length - dot)) {
    str = str + pad.substring(str.length - dot);
  } else {
    str = str.substring(0, dot + pad.length);
  }
  return str + ' MB';
}

function updateQuota() {
  var now = (new Date()).getTime();

	var quota = document.getElementById("quota");

	// Don't do anything, in case page doesn't have counter control
	if (!quota) {
		return;
	}

  quota.innerHTML = formatSpace(startQuota + ((now - startTime) / 1000) * (extraStoragePerDay / (24.0 * 60 * 60)), '.000000');

  setTimeout(updateQuota, 1000);
}

function onLoad()
{
  updateQuota();
}

</script>
<!-- End of storage quota counter code -->

