	var url20b = 'https://web.archive.org/web/20070629231533/http://downloads.sourceforge.net/phpbb/phpBB-2.0.22';

	var files20 = 'https://web.archive.org/web/20070629231533/http://downloads.sourceforge.net/phpbb/phpBB-2.0.22-files';
	var patch20 = 'https://web.archive.org/web/20070629231533/http://downloads.sourceforge.net/phpbb/phpBB-2.0.22-patch';
	var changes20 = 'https://web.archive.org/web/20070629231533/http://downloads.sourceforge.net/phpbb/phpBB-2.0.22-codechanges';

	function change_format(id, format) {

		var url;

		if(id == 'download30')
		{
			url = document.getElementById('download30');
			url.href = url30 + '.' + format;
			
			url = document.getElementById('download30b');
			url.href = url30b + '.' + format;			

			url = document.getElementById('files30');
			url.href = files30 + '.' + format;

			url = document.getElementById('patch30');
			url.href = patch30 + '.' + format;

			url = document.getElementById('update30');
			url.href = update30 + '.' + format;
		}
		else if(id == 'download20')
		{
			url = document.getElementById('download20');
			url.href = url20 + '.' + format;
			
			url = document.getElementById('download20b');
			url.href = url20b + '.' + format;

			url = document.getElementById('files20');
			url.href = files20 + '.' + format;

			url = document.getElementById('patch20');
			url.href = patch20 + '.' + format;

			url = document.getElementById('changes20');
			url.href = changes20 + '.' + format;
		}
	}

// ]]>
</script>


