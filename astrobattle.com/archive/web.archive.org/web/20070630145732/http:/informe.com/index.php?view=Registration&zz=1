    function getPaths()
    {
        var images = new Array(32);
                    images['subSilver'] = 'subSilver';
                    images['Artemis Lite'] = 'ArtemisLite';
                    images['Artemis Lite (Blue)'] = 'ArtemisLiteBlue';
                    images['Artemis Lite (Green)'] = 'ArtemisLiteGreen';
                    images['metalBB'] = 'metalBB';
                    images['abandon'] = 'abandon';
                    images['mediocre'] = 'mediocre';
                    images['GreenTech'] = 'GreenTech';
                    images['GorbGreenII'] = 'GorbGreenII';
                    images['Acid'] = 'Acid';
                    images['Diddle'] = 'Diddle';
                    images['subLite'] = 'subLite';
                    images['Neon'] = 'Neon';
                    images['aluminoid'] = 'aluminoid';
                    images['kdm2'] = 'kdm2';
                    images['subGreen'] = 'subGreen';
                    images['Getaway'] = 'Getaway';
                    images['PurplePearl_C'] = 'PurplePearl_C';
                    images['lunazstyle'] = 'lunazstyle';
                    images['purplePulseV2'] = 'purplePulseV2';
                    images['BBTech'] = 'BBTech';
                    images['Saphic'] = 'Saphic';
                    images['DAJ_Glass'] = 'DAJ_Glass';
                    images['Blob'] = 'Blob';
                    images['Minimalistic'] = 'Minimalistic';
                    images['RedSilver'] = 'RedSilver';
                    images['chairs'] = 'chairs';
                    images['GetawayPurple'] = 'GetawayPurple';
                    images['Classic'] = 'Classic';
                    images['Jet'] = 'Jet';
                    images['jasidogdotcom'] = 'jasidogdotcom';
                    images['xand'] = 'xand';
                    images['INFORME Style'] = 'informe_com';
                return images;
    }
    function setSrc()
    {
        var images       = getPaths();
        var objectSelect = document.getElementById('themeSelector');
        var objectImage  = document.getElementById('previewImage');
        var previewPath  = 'https://web.archive.org/web/20070629034026/http://www.informe.com/pix/thumbs/' + images[objectSelect.options[objectSelect.selectedIndex].label] + '.jpg';
        objectImage.src = previewPath;
    }
    window.onload = setSrc;
</script></div>
