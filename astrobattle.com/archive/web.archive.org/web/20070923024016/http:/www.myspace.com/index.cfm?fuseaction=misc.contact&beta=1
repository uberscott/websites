<script language="javascript" type="text/javascript">
function ClientCallbackScript(result, context)
{
    //fix for Safari 2.0.4
  var ddlSubCategory = document.getElementById('ctl00_cpMain_HelpCenter_ContactUs1_Ddl_SubCategories');

  ddlSubCategory.innerHTML= "";

  var subcategories = result.split(';');
  for (var i = 0; i < subcategories.length - 1; ++i)
  {
    var properties = subcategories[i].split('|');
    var SubCategoryName = properties[0];
    var SubCategoryId = properties[1];
    

    var option = document.createElement("option");
    option.value = SubCategoryId;
    option.innerHTML = SubCategoryName;
    ddlSubCategory.appendChild(option);
  }
}

function SubmitForm()
{

    var ddlCategory = document.getElementById('ctl00_cpMain_HelpCenter_ContactUs1_Ddl_FaqCategories');
    var Categoryindex  = ddlCategory.selectedIndex
	var CategorySelValue = ddlCategory.options[Categoryindex].value
  
    var ddlSubCategory = document.getElementById('ctl00_cpMain_HelpCenter_ContactUs1_Ddl_SubCategories');
  
	var SubCategoryindex  = ddlSubCategory.selectedIndex
	var SubCategorySelValue = ddlSubCategory.options[SubCategoryindex].value
	
	if(Categoryindex == 0)
	{
	     alert('Selecciona un tema');
	     return;
    }
	   
	if(Categoryindex != 0 && SubCategoryindex ==0)
	{
	     alert('Selecciona un subtema');
  	    return;
	}
	   
	window.location='https://web.archive.org/web/20071107192829/http://www.myspace.com/index.cfm?fuseaction=misc.relatedfaqs&Category='+CategorySelValue+'&SubCategory='+SubCategorySelValue;     
}


</script>

