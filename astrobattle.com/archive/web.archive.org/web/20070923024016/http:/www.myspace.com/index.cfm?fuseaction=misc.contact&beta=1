<script type="text/javascript">
	//comes from ColdFusion version
		// This script encodes the email address so it can be picked up easily.
		// This script is (C) Copyright 2004 Jim Tucek
		// Leave these comments alone!  For more info, visit
		// www.jracademy.com/~jtucek/email/ 

		function knit(forest,design,fuel) {

			forest += ' ';
			var oxygen = forest.length;
			var corporation = 0;
			var walkway = '';
			for(var circumstance = 0; circumstance < oxygen; circumstance++) {
				corporation = 0;
				while(forest.charCodeAt(circumstance) != 32) {
					corporation = corporation * 10;
					corporation = corporation + forest.charCodeAt(circumstance)-48;
					circumstance++;
				}
				walkway += String.fromCharCode(spill(corporation,design,fuel));
			}
			parent.location = 'm'+'a'+'i'+'l'+'t'+'o'+':'+walkway;
		}


		function spill(rocket,language,matrix) {
			if (matrix % 2 == 0) {
				place = 1;
				for(var birth = 1; birth <= matrix/2; birth++) {
					information = (rocket*rocket) % language;
					place = (information*place) % language;
				}
			} else {
				place = rocket;
				for(var qualification = 1; qualification <= matrix/2; qualification++) {
					information = (rocket*rocket) % language;
					place = (information*place) % language;
				}
			}
			return place;
		}


		function spill(rocket,language,matrix) {
			if (matrix % 2 == 0) {
				place = 1;
				for(var birth = 1; birth <= matrix/2; birth++) {
					information = (rocket*rocket) % language;
					place = (information*place) % language;
				}
			} else {
				place = rocket;
				for(var qualification = 1; qualification <= matrix/2; qualification++) {
					information = (rocket*rocket) % language;
					place = (information*place) % language;
				}
			}
			return place;
		}
</script>         
