<script language="javascript" type="text/javascript">
function ClientCallbackScript(result, context)
{
    //fix for Safari 2.0.4
  var ddlSubCategory = document.getElementById('ctl00_cpMain_HelpCenter_ContactUs1_Ddl_SubCategories');

  ddlSubCategory.innerHTML= "";

  var subcategories = result.split(';');
  for (var i = 0; i < subcategories.length - 1; ++i)
  {
    var properties = subcategories[i].split('|');
    var SubCategoryName = properties[0];
    var SubCategoryId = properties[1];
    

    var option = document.createElement("option");
    option.value = SubCategoryId;
    option.innerHTML = SubCategoryName;
    ddlSubCategory.appendChild(option);
  }
}

